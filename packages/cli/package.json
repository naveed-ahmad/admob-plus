{
  "name": "admob-plus",
  "description": "AdMob Plus CLI",
  "version": "0.15.8",
  "bin": "./bin/run",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "clipboardy": "^2.3.0",
    "elementtree": "^0.1.7",
    "envinfo": "^7.7.3",
    "execa": "^4.1.0",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.20",
    "read-pkg-up": "^7.0.1",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/clipboardy": "^2.0.1",
    "@types/elementtree": "^0.1.0",
    "@types/fs-extra": "^9.0.4",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14",
    "chai": "^4",
    "globby": "^11",
    "mocha": "^8.2.1",
    "nyc": "^15",
    "ts-node": "^9",
    "tslint": "^6",
    "typescript": "^4.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "author": "Ratson",
  "license": "MIT",
  "repository": "admob-plus/admob-plus",
  "homepage": "https://admob-plus.github.io",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "admob-plus",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "mocha": {
    "require": "ts-node/register",
    "watch-extensions": "ts",
    "recursive": true,
    "reporter": "spec",
    "timeout": 30000
  }
}
